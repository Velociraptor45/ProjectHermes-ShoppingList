{
  "Kestrel": {
    "IP-Address": "0.0.0.0",
    "Port": 443
  },
  "Certificate": {
    "CrtFilePath": "ssl/shoppinglist-api.crt",
    "PrivateKeyFilePath": "ssl/shoppinglist-api.key"
  },
  "KeyVault": {
    "Paths": {
      "ConnectionStrings": "ConnectionStrings"
    },
    "MountPoint": "ph-shoppinglist-api",
    "Uri": "https://{address}:14777"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "\\app\\logs\\prd-ph-sl-api-logs.log",
          "rollOnFileSizeLimit": true,
          "fileSizeLimitBytes": 1048576,
          "retainedFileCountLimit": 20
        }
      }
    ]
  }
}